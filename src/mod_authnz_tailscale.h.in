#ifndef mod_authnz_tailscale_h
#define mod_authnz_tailscale_h

#undef PACKAGE_VERSION
#undef PACKAGE_TARNAME
#undef PACKAGE_STRING
#undef PACKAGE_NAME
#undef PACKAGE_BUGREPORT

/**
 * @macro
 * Version number of the md module as c string
 */
#define MOD_AUTHNZ_TAILSCALE_VERSION "@PACKAGE_VERSION@-git"

/* user agent for requests */
#define MOD_AUTHNZ_TAILSCALE_UA      "Apache mod_authnz_tailscale "##MOD_AUTHNZ_TAILSCALE_VERSION

/**
 * @macro
 * Numerical representation of the version number of the module
 * release. This is a 24 bit number with 8 bits for major number, 8 bits
 * for minor and 8 bits for patch. Version 1.2.3 becomes 0x010203.
 */
#define MOD_AUTHNZ_TAILSCALE_VERSION_NUM @PACKAGE_VERSION_NUM@

#define TAILSCALE_DEF_URL    "@TAILSCALE_DEF_URL@"

#endif /* mod_authnz_tailscale_h */
